{
  "name": "backend",
  "version": "1.0.0",
  "description": "MyUSafe Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node src/scripts/seedData.js",
    "test": "mocha tests/**/*.test.js --require tests/setup.js --timeout 10000 --exit",
    "test:watch": "mocha tests/**/*.test.js --require tests/setup.js --watch",
