    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "mochawesome": "^7.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "complaint",
    "university",
    "backend"
  ],
